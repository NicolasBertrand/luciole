style "Tropical" 
{ 
  
    fg[NORMAL]        = '#101010'
	fg[PRELIGHT]      = '#101010'
	fg[SELECTED]      = '#1A1A1A'
	fg[ACTIVE]        = '#101010'
	fg[INSENSITIVE]   = darker ('#EDEDED')

	bg[NORMAL]        = '#EDEDED'
	bg[PRELIGHT]      = shade (1.02, '#EDEDED')
	bg[SELECTED]	  = '#9DDDC8'  
	bg[INSENSITIVE]   = '#EDEDED'
	bg[ACTIVE]        = shade (0.90, '#EDEDED')

	base[NORMAL]      = '#fff'
	base[PRELIGHT]    = shade (0.95, '#EDEDED')
	base[ACTIVE]      = shade (0.90, '#EDEDED')
	base[SELECTED]    = shade (1.25, '#BFF44B')  
	base[INSENSITIVE] = '#EDEDED'

	text[NORMAL]      = '#1A1A1A'
	text[PRELIGHT]    = '#1A1A1A'
	text[ACTIVE]      = '#1A1A1A'
	text[SELECTED]    = '#1A1A1A'
	text[INSENSITIVE] = darker ('#EDEDED')

 stock["gtk-go-forward"] = 
  {
    { "Tropical/icons/gtk-go-forward-ltr.png", *, *, * }
  }
  
  stock["gtk-go-up"] = 
  {
    { "Tropical/icons/gtk-go-up.png", *, *, * }
  }
  
  stock["gtk-go-down"] = 
  {
    { "Tropical/icons/gtk-go-down.png", *, *, * }
  }
  
  stock["gtk-open"] = 
  {
